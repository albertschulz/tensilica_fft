// DIT Butterfly Compute Node
function [63:0] calc_butterfly_dit ([31:0] even, [31:0] odd, [31:0] twiddle, [0:0] shift)
{
	wire [15:0] wr 		= real(twiddle);
	wire [15:0] wi 		= imag(twiddle);
	wire [15:0] even_r	= TIEmux(shift, real(even), {1{even[31]}, even[31:17]});
	wire [15:0] even_i	= TIEmux(shift, imag(even), {1{even[15]}, even[15:1]});
	
	wire [15:0] odd_r_wr	= FixedMul(wr, real(odd));
	wire [15:0] odd_i_wi	= FixedMul(wi, imag(odd));
	wire [15:0] temp_r		= FixedSub(odd_r_wr, odd_i_wi);
	
	wire [15:0] odd_r_wi 	= FixedMul(wi, real(odd));
	wire [15:0] odd_i_wr 	= FixedMul(wr, imag(odd));
	wire [15:0] temp_i 		= TIEadd(odd_r_wi, odd_i_wr, 1'b0);
	
	wire [15:0] even_out_r = TIEadd( even_r, temp_r, 1'b0);
	wire [15:0] even_out_i = TIEadd( even_i, temp_i, 1'b0);
	wire [15:0] odd_out_r  = FixedSub(even_r, temp_r); 
	wire [15:0] odd_out_i  = FixedSub(even_i, temp_i); 
	
	wire [31:0] even_out 		= {even_out_r, even_out_i};
	wire [31:0] odd_out			= {odd_out_r, odd_out_i};
	
	assign calc_butterfly_dit = {even_out, odd_out};
}

// DIF Butterfly Compute Node
function [63:0] calc_butterfly_dif ([31:0] even, [31:0] odd, [31:0] twiddle, [0:0] shift)
{
	wire [15:0] wr 		= real(twiddle);
	wire [15:0] wi 		= imag(twiddle);
	wire [15:0] even_r	= real(even);
	wire [15:0] even_i	= imag(even);

	wire [15:0] c_e = FixedSub(even_r, real(odd));
	wire [15:0] f_d = FixedSub(imag(odd),even_i);
	
	wire [15:0] c_e_wr = FixedMul(c_e, wr);
	wire [15:0] f_d_wi = FixedMul(f_d, wi);
	wire [15:0] c_e_wi = FixedMul(c_e, wi);
	wire [15:0] f_d_wr = FixedMul(f_d, wr);
	
	wire [15:0] even_out_r = TIEadd( even_r, real(odd), 1'b0);
	wire [15:0] even_out_i = TIEadd( even_i, imag(odd), 1'b0);
	
	wire [15:0] even_out_r_shifted = TIEmux(shift[0], even_out_r, {even_out_r[15], even_out_r[15:1]});
	wire [15:0] even_out_i_shifted = TIEmux(shift[0], even_out_i, {even_out_i[15], even_out_i[15:1]});
	
	
	wire [15:0] odd_out_r  = TIEadd(c_e_wr, f_d_wi, 1'b0); 
	wire [15:0] odd_out_i  = FixedSub(c_e_wi, f_d_wr); 
	
	wire [31:0] even_out 		= {even_out_r_shifted, even_out_i_shifted};
	wire [31:0] odd_out			= {odd_out_r, odd_out_i};
		
	assign calc_butterfly_dif = {even_out, odd_out};
}

function [255:0] calc_4_butterflies_dif([63:0] qr, [63:0] qi, [63:0] fr, [63:0] fi, [127:0] tw_vec, [0:0] shift) shared
{
	wire [31:0] tw1 = tw_vec[127:96];
	wire [31:0] tw2 = tw_vec[95:64];
	wire [31:0] tw3 = tw_vec[63:32];
	wire [31:0] tw4 = tw_vec[31:0];
	
	wire [63:0] result1 = calc_butterfly_dif({qr[63:48],qi[63:48]}, {fr[63:48],fi[63:48]}, tw1, shift);
	wire [63:0] result2 = calc_butterfly_dif({qr[47:32],qi[47:32]}, {fr[47:32],fi[47:32]}, tw2, shift);
	wire [63:0] result3 = calc_butterfly_dif({qr[31:16],qi[31:16]}, {fr[31:16],fi[31:16]}, tw3, shift);
	wire [63:0] result4 = calc_butterfly_dif({qr[15:0 ],qi[15:0 ]}, {fr[15:0 ],fi[15:0 ]}, tw4, shift);
	
	assign calc_4_butterflies_dif = {result1, result2, result3, result4};
}

function [255:0] calc_4_butterflies_dit([63:0] qr, [63:0] qi, [63:0] fr, [63:0] fi, [127:0] tw_vec, [0:0] shift) shared
{
	wire [31:0] tw1 = tw_vec[127:96];
	wire [31:0] tw2 = tw_vec[95:64];
	wire [31:0] tw3 = tw_vec[63:32];
	wire [31:0] tw4 = tw_vec[31:0];
	
	wire [63:0] result1 = calc_butterfly_dit({qr[63:48],qi[63:48]}, {fr[63:48],fi[63:48]}, tw1, shift);
	wire [63:0] result2 = calc_butterfly_dit({qr[47:32],qi[47:32]}, {fr[47:32],fi[47:32]}, tw2, shift);
	wire [63:0] result3 = calc_butterfly_dit({qr[31:16],qi[31:16]}, {fr[31:16],fi[31:16]}, tw3, shift);
	wire [63:0] result4 = calc_butterfly_dit({qr[15:0 ],qi[15:0 ]}, {fr[15:0 ],fi[15:0 ]}, tw4, shift);
	
	assign calc_4_butterflies_dit = {result1, result2, result3, result4};
}

operation FFT_DIT_CALC_FIRST_STAGE
{in AR k, in BR inverse, in BR shift, inout vect8x16 real, inout vect8x16 imag} {}
{
	wire [63:0] even_r 	= get_even(real);
	wire [63:0] even_i 	= get_even(imag);
	wire [63:0] odd_r 	= get_odd(real);
	wire [63:0] odd_i 	= get_odd(imag);
	
	wire [127:0] twiddle_factors = calc_4_twiddle_factors(0, k, inverse, shift);
	
	wire [31:0] twiddle = twiddle_factors[127:96];
	
	wire [127:0] tw_vector = {twiddle, twiddle, twiddle, twiddle};
		
	wire [255:0] bf_results = calc_4_butterflies_dit(even_r, even_i, odd_r, odd_i, tw_vector, shift);
	wire [127:0] imagValues 	= imagFrom256(bf_results);
	wire [127:0] realValues 	= realFrom256(bf_results);

	assign real = reverse_shuffle8x16(realValues);
	assign imag = reverse_shuffle8x16(imagValues);
}

