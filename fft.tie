format flix64 64 {slot_0, slot_1, slot_2}
slot_opcodes slot_0 {FFT_SIMD_STORE_SHUFFLED, L32I, L16SI, S16I, FFT_SIMD_LOAD}
slot_opcodes slot_1 {ADD.N, ADDX2}
slot_opcodes slot_2 {FFT_SIMD_STORE_SHUFFLED, L32I, L16SI, S16I, FFT_SIMD_LOAD}

operation FFT_CHECK_SHIFT_CONDITION {in vect8x16 j_vector, in vect8x16 m_vector, out BR should_shift} {}
{
	wire [15:0] j1 = abs(j_vector[127:112]);
	wire [15:0] j2 = abs(j_vector[111:96 ]);
	wire [15:0] j3 = abs(j_vector[95 :80 ]);
	wire [15:0] j4 = abs(j_vector[79 :64 ]);
	wire [15:0] j5 = abs(j_vector[63 :48 ]);
	wire [15:0] j6 = abs(j_vector[47 :32 ]);
	wire [15:0] j7 = abs(j_vector[31 :16 ]);
	wire [15:0] j8 = abs(j_vector[15 :0  ]);
	
	wire [15:0] m1 = abs(m_vector[127:112]);
	wire [15:0] m2 = abs(m_vector[111:96 ]);
	wire [15:0] m3 = abs(m_vector[95 :80 ]);
	wire [15:0] m4 = abs(m_vector[79 :64 ]);
	wire [15:0] m5 = abs(m_vector[63 :48 ]);
	wire [15:0] m6 = abs(m_vector[47 :32 ]);
	wire [15:0] m7 = abs(m_vector[31 :16 ]);
	wire [15:0] m8 = abs(m_vector[15 :0  ]);
	
	assign should_shift = 		greaterThan16383(j1) 
							| 	greaterThan16383(j2) 
							| 	greaterThan16383(j3) 
							| 	greaterThan16383(j4) 
							| 	greaterThan16383(j5) 
							| 	greaterThan16383(j6) 
							| 	greaterThan16383(j7) 
							| 	greaterThan16383(j8) 
							| 	greaterThan16383(m1) 
							| 	greaterThan16383(m2) 
							| 	greaterThan16383(m3) 
							| 	greaterThan16383(m4) 
							| 	greaterThan16383(m5) 
							| 	greaterThan16383(m6) 
							| 	greaterThan16383(m7) 
							| 	greaterThan16383(m8);
}

operation FFT_SIMD_LOAD_SHUFFLED
{in AR *addr, in AR offset, out vect8x16 output, in BR reverse} 
{out VAddr, in MemDataIn128}
{
	assign VAddr =  calc_addr(addr, offset);
	wire [127:0] reverseDataIn = reverse_2_byte_wise(MemDataIn128);
	assign output = TIEmux(reverse, shuffle8x16(reverseDataIn), reverse_shuffle8x16(reverseDataIn));
}

operation FFT_SIMD_STORE_SHUFFLED
{in AR *addr, in AR offset, in vect8x16 input, in BR reverse} 
{out VAddr, out MemDataOut128}
{
	assign VAddr 				= calc_addr(addr, offset);
	wire [127:0] shuffledValues = TIEmux(reverse, shuffle8x16(input), reverse_shuffle8x16(input));
	assign MemDataOut128 		= reverse_2_byte_wise(shuffledValues);
}

operation FFT_SIMD_LOAD 
{in AR *addr, in AR offset, out vect8x16 output} 
{out VAddr, in MemDataIn128}
{
	assign VAddr = calc_addr(addr, offset);
	assign output = reverse_2_byte_wise(MemDataIn128);
}

operation FFT_SIMD_STORE 
{in AR *addr, in AR offset, in vect8x16 data} 
{out VAddr, out MemDataOut128}
{
	assign VAddr 				= calc_addr(addr, offset);
	assign MemDataOut128 		= reverse_2_byte_wise(data);
}

//
// Reads 128 bit from memory
// Assigns upper 64 bits to vector 1, and
// lower 64 bits to vector 2
// By setting the flag load_upper you can control whether the 64 bits should be stored 
// in the upper or lower location of the vectors
operation FFT_SIMD_LOAD_INTERLEAVED 
{in AR *addr, in AR offset, inout vect8x16 vec1, inout vect8x16 vec2, in BR load_upper} 
{out VAddr, in MemDataIn128}
{
	assign VAddr = calc_addr(addr, offset);
	wire [127:0] mem = reverse_2_byte_wise(MemDataIn128);
	assign vec1 = TIEmux(load_upper, {vec1[127:64], mem[127:64]}, {mem[127:64], vec1[63:0]});
	assign vec2 = TIEmux(load_upper, {vec2[127:64], mem[63:0]}, {mem[63:0], vec2[63:0]});
}

operation FFT_SIMD_STORE_INTERLEAVED 
{in AR *addr, in AR offset, in vect8x16 vec1, in vect8x16 vec2, in BR store_upper} 
{out VAddr, out MemDataOut128}
{
	assign VAddr = calc_addr(addr, offset);
		
	wire [63:0] data1 = TIEmux(store_upper, lower(vec1), upper(vec1));
	wire [63:0] data2 = TIEmux(store_upper, lower(vec2), upper(vec2));
		
	wire [127:0] data = {data1, data2};
	assign MemDataOut128 = reverse_2_byte_wise(data);
}


